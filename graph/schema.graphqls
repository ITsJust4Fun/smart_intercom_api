type Video {
  _id: ID!
  time: String!
  link: String!
}

type Query {
  videos: [Video!]!
}

input NewVideo {
  time: String!
  link: String!
}

input RefreshTokenInput{
  token: String!
}

input Login {
  password: String!
}

input NewPassword {
  passwordNew: String!
  passwordOld: String!
}

type Mutation {
  login(input: Login!): String!
  changePassword(input: NewPassword!): String!
  refreshToken(input: RefreshTokenInput!): String!
  createVideo(input: NewVideo!): Video!
}
